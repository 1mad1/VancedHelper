{
	"name": "vanced-helper",
	"version": "2.0.0",
	"description": "",
	"author": "Mattis6666",
	"license": "AGPL-3.0",
	"main": "dist/index.js",
	"engines": {
		"node": "13.6.0"
	},
	"eslintIgnore": [
		"dist",
		".vscode"
	],
	"scripts": {
		"start": "npm run clean & npm run build && export NODE_ENV=production && node .",
		"dev": "npm run build && export NODE_ENV=development && node .",
		"prod": "npm run clean & npm run build && export NODE_ENV=production && node .",
		"test": "tsc -p tsconfig.test.json && npm run lint",
		"lint": "eslint ./src/**/*.ts --fix",
		"clean": "rm -rf dist/*",
		"reinstall": "npm cache clean --force && rm -rf node_modules && rm yarn.lock && yarn install",
		"build": "tsc",
		"push": "func() { git add . && git commit -m \"$1\" && git push; }; func"
	},
	"husky": {
		"hooks": {
			"pre-commit": "precise-commits && npm run test"
		}
	},
	"dependencies": {
		"@discordjs/opus": "^0.3.2",
		"canvas": "^2.6.1",
		"common-tags": "^1.8.0",
		"discord.js": "^12.0.2",
		"mongoose": "^5.9.19",
		"node-fetch": "^2.6.0",
		"ordinal": "^1.0.3",
		"tinycolor2": "^1.4.1",
		"ytdl-core-discord": "^1.2.1"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.0",
		"@types/mongoose": "^5.7.27",
		"@types/node": "^14.0.13",
		"@types/node-fetch": "^2.5.7",
		"@types/tinycolor2": "^1.4.2",
		"@types/ws": "^7.2.5",
		"@typescript-eslint/eslint-plugin": "^3.3.0",
		"@typescript-eslint/parser": "^3.3.0",
		"eslint": "^7.3.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.2.5",
		"precise-commits": "^1.0.2",
		"prettier": "^2.0.5",
		"typescript": "^3.9.5"
	}
}
